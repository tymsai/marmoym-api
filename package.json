  "description": "tymsai-api",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./lib/*",
    "dev": "yarn run clean && yarn run watch",
    "watch": "./node_modules/typescript/bin/tsc -w -p ./internals/typescript/tsconfig.dev.json",
    "build": "./node_modules/typescript/bin/tsc -p ./internals/typescript/tsconfig.dev.json",
    "start": "NODE_ENV=development nodemon ./lib/app.js",
    "start:debug": "NODE_ENV=development nodemon ./lib/app.js --debug",
    "migrate:dev": "NODE_ENV=development node ./src/database/migrate.js",
    "migrate:prod": "NODE_ENV=production node ./src/database/migrate.js",
    "seed:dev": "NODE_ENV=production node ./src/database/seed.js",
    "rollback:dev": "node ./src/database/rollback.js",
    "setup:dev": "./internals/shell/setup.sh",
    "test": "echo \"Error: no test specified\""
