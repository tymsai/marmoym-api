{
  "name": "marmoym-api",
  "version": "0.0.1",
  "description": "marmoym-api",
  "scripts": {
    "babel": "./node_modules/.bin/babel ./tsc_out --out-dir ./dist",
    "build": "yarn run clean && yarn run tsc && yarn run babel",
    "clean": "./node_modules/.bin/rimraf ./dist/* ./tsc_out/*",
    "lint": " ./node_modules/.bin/tslint -t stylish -p ./tsconfig.json",
    "migrate:dev": "yarn run build && NODE_ENV=development node ./dist/database/setup.js --migrate",
    "migrate:prod": "yarn run build && NODE_ENV=production node ./dist/database/setup.js --migrate",
    "pm2:deploy": "pm2 deploy ./internals/pm2/ecosystem.config.js production",
    "pm2:start:prod": "yarn run build && pm2 startOrRestart ./internals/pm2/ecosystem.config.js --env production",
    "preinstall:dev": "./internals/shell/preinstall.sh",
    "rollback:dev": "yarn run build && NODE_ENV=development node ./dist/database/setup.js --rollback",
    "seed:dev": "yarn run build && NODE_ENV=development node ./dist/database/setup.js --seed",
    "start:dev": "yarn run build && NODE_ENV=development node ./bin/www",
    "start:prod": "yarn run build && NODE_ENV=production node ./bin/www",
    "start:dev:debug": "NODE_ENV=development node ./dist/app.js --debug",
    "test": "echo \"Error: no test specified\"",
    "tsc": "./node_modules/typescript/bin/tsc -p ./tsconfig.json"
  },
  "author": "tymsai",
  "license": "ISC",
  "devDependencies": {
    "@types/body-parser": "^1.16.3",
    "@types/express": "^4.0.35",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.14",
    "@types/sequelize": "^4.0.51",
    "babel-cli": "^6.26.0",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.0.2",
    "mocha": "^3.4.2",
    "morgan": "^1.9.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.8.0",
    "typescript": "^2.5.3"
  },
  "dependencies": {
    "bcrypt": "^1.0.3",
    "body-parser": "^1.17.1",
    "cors": "^2.8.4",
    "express": "^4.15.2",
    "jsonwebtoken": "^7.4.0",
    "knex": "^0.13.0",
    "mariasql": "^0.2.6",
    "mysql": "^2.14.1",
    "mysql2": "^1.4.2",
    "pg": "^7.4.1",
    "sequelize": "^3.30.4",
    "winston": "^3.0.0-rc4",
    "winston-daily-rotate-file": "^2.0.0-rc.6"
  }
}
